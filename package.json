{
  "name": "layr",
  "displayName": "Layr - AI Planning Layer",
  "description": "AI-powered project planning that just works! Pre-configured with Groq - install and start planning instantly. Zero configuration required.",
  "version": "2.0.3",
  "publisher": "ManasDutta",
  "icon": "layr.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/manasdutta04/layr"
  },
  "pricing": "Free",
  "sponsor": {
    "url": "https://github.com/sponsors/manasdutta04"
  },
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "AI",
    "Keymaps",
    "Other"
  ],
  "activationEvents": [
    "onCommand:layr.createPlan",
    "onCommand:layr.executePlan",
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "layr.createPlan",
        "title": "Create Plan",
        "category": "Layr"
      },
      {
        "command": "layr.executePlan",
        "title": "Execute Plan",
        "category": "Layr"
      }
    ],
    "configuration": {
      "title": "Layr",
      "properties": {}
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./"
  },
  "devDependencies": {
    "@types/node": "16.x",
    "@types/vscode": "^1.74.0",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@google/generative-ai": "^0.2.1",
    "@types/node-fetch": "^2.6.13",
    "dotenv": "^17.2.3",
    "node-fetch": "^3.3.2"
  }
}
